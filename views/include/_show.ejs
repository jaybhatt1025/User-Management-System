<% for(var i=0;i<users.length;i++) { %>
<tr>
    <td><%= i + 1%></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
      <a href="/update-user?id=<%= users[i]._id %>">
        <span class="mx-auto me-4"><i class="fas fa-pencil-alt"></i></span>
      </a>
      <a class="delete" data-id=<%= users[i]._id%> >
        <span><i class="fas fa-times"></i></span>
      </a>
    </td>
  </tr>

  <% } %>